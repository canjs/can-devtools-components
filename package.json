{
  "name": "can-devtools-components",
  "version": "0.0.4",
  "description": "Components for CanJS Devtools",
  "homepage": "https://canjs.com/",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-devtools-components.git"
  },
  "author": {
    "name": "Bitovi",
    "email": "contact@bitovi.com",
    "url": "https://bitovi.com"
  },
  "scripts": {
    "preversion": "npm test && npm run build",
    "version": "git commit -am \"Update version number\" && git checkout -b release && git add -f dist/",
    "postpublish": "git push --tags && git checkout - && git branch -D release && git push",
    "testee": "testee test.html --browsers firefox",
    "test": "npm run jshint && npm run testee",
    "jshint": "jshint ./*.js  --config",
    "release:pre": "npm version patch && npm publish --tag pre",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "build": "node build.js && lessc can-devtools-components.less dist/can-devtools-components.css"
  },
  "main": "dist/can-devtools-components",
  "keywords": [
    "donejs-plugin"
  ],
  "steal": {
    "main": "can-devtools-components",
    "npmIgnore": [
      "donejs-cli",
      "generator-donejs",
      "testee",
      "steal-tools"
    ],
    "plugins": [
      "steal-less",
      "steal-stache",
      "steal-css"
    ],
    "envs": {
      "build-development": {
        "map": {
          "steal-less": "make-empty"
        }
      }
    },
    "map": {
      "chai": "chai/chai"
    }
  },
  "dependencies": {
    "can-component": "^4.0.2",
    "can-debug": "^1.0.2",
    "can-dom-events": "^1.2.0",
    "can-event-dom-enter": "^2.1.2",
    "can-reflect": "^1.15.2",
    "can-view-import": "^4.0.2",
    "spectre-canjs": "^2.3.0",
    "spectre.css": "^0.5.1"
  },
  "devDependencies": {
    "can-define": "^2.1.2",
    "chai": "^4.1.2",
    "donejs-cli": "^2.1.0",
    "generator-donejs": "^2.1.0",
    "jshint": "^2.9.1",
    "less": "^3.0.1",
    "steal": "^1.6.5",
    "steal-css": "^1.3.2",
    "steal-less": "^1.2.2",
    "steal-mocha": "^1.0.0",
    "steal-qunit": "^1.0.1",
    "steal-stache": "^4.1.0",
    "steal-tools": "^1.11.3",
    "testee": "^0.7.0"
  },
  "license": "MIT"
}
